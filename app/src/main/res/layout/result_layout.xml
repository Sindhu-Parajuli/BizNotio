<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/first">


    <RelativeLayout android:layout_width="669dp" android:layout_height="336dp"
                    tools:ignore="MissingConstraints" app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="204dp" tools:layout_editor_absoluteX="63dp"

        android:background="@drawable/frame">

    </RelativeLayout>
    <ImageView
            android:id="@+id/user_profile_image_search"
            android:layout_width="663dp"
            android:layout_height="175dp"
            android:src="@drawable/icons"
            android:padding="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="792dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.501"/>

        <TextView
                android:id="@+id/username"
                android:layout_width="171dp"
                android:layout_height="70dp"
                android:text="user name"
                android:gravity="center"
                android:textSize="12dp"
                tools:ignore="MissingConstraints"
                android:layout_marginTop="9dp"
                app:layout_constraintTop_toBottomOf="@+id/user_profile_image_search"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="315dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="314dp"
                android:layout_marginBottom="713dp" app:layout_constraintBottom_toBottomOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>