<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/v"
        tools:context="Fragments.ProfileFragment">


  <ImageView
          android:id="@+id/imageView"
          android:layout_width="114dp"
          android:layout_height="114dp"
          android:layout_marginStart="4dp"
          android:layout_marginTop="44dp"
          android:layout_marginEnd="4dp"
          android:layout_marginBottom="501dp"
          android:src="@drawable/profile"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"

          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent"
          android:clickable="true"
          app:layout_constraintHorizontal_bias="0.235" app:layout_constraintVertical_bias="0.43"/>
  <TextView
          android:text="NAME"
          android:layout_width="141dp"
          android:layout_height="37dp" android:id="@+id/textView"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.318"
          app:layout_constraintVertical_bias="0.302" android:textSize="18sp"/>
  <ImageButton
            android:id="@+id/imagelogoutbutton"
            android:background="#0B0A0A"

            android:src="@drawable/tinys" tools:ignore="MissingConstraints" android:layout_height="32dp"
            android:layout_width="41dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintVertical_bias="0.94"/>
  <Button
          android:text="Edit Profile"

          android:layout_width="137dp"
          android:layout_height="50dp" android:id="@+id/edit_button"
          app:layout_constraintBottom_toBottomOf="@+id/imagelogoutbutton"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintVertical_bias="0.295" app:layout_constraintHorizontal_bias="0.872"/>

  <TextView
          android:text="Interests"
          android:layout_width="358dp"
          android:layout_height="58dp" android:id="@+id/Interests"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintVertical_bias="0.692" app:layout_constraintHorizontal_bias="0.698"/>
  <TextView
          android:text="Profession"
          android:layout_width="344dp"
          android:layout_height="60dp" android:id="@+id/profession"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.597"
          app:layout_constraintVertical_bias="0.438"/>
  <TextView
          android:text="Education"
          android:layout_width="339dp"
          android:layout_height="55dp" android:id="@+id/Education"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintVertical_bias="0.559" app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintHorizontal_bias="0.513"/>
  <TextView
          android:text="BizNotio Goals"
          android:layout_width="336dp"
          android:layout_height="60dp" android:id="@+id/Goals"
          android:layout_marginTop="26dp"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintVertical_bias="0.824" app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintHorizontal_bias="0.533"/>
  <de.hdodenhof.circleimageview.CircleImageView
          android:id="@+id/circle_image_profile"
          android:layout_width="114dp"
          android:layout_height="114dp"
          android:layout_marginEnd="8dp"
          app:layout_constraintBottom_toBottomOf="@+id/profile_image_clickable"
          app:layout_constraintEnd_toEndOf="@+id/imageView"
          app:layout_constraintStart_toStartOf="@+id/profile_image_clickable"
          app:layout_constraintTop_toTopOf="@+id/imageView" android:layout_marginTop="4dp"/>
  <TextView
          android:text="0"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" android:id="@+id/numberofconnections"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.735"
          app:layout_constraintVertical_bias="0.123"/>
  <TextView
          android:text="Connections"
          android:layout_width="130dp"
          android:layout_height="50dp" android:id="@+id/connection"
          app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintHorizontal_bias="0.875" app:layout_constraintVertical_bias="0.18"
          android:textColor="#3F51B5" android:textSize="18sp"/>


</androidx.constraintlayout.widget.ConstraintLayout>
