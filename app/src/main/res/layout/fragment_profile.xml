<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/soft"
        tools:context="Fragments.ProfileFragment"
        android:orientation="vertical" android:id="@+id/linearLayout">



    <FrameLayout android:layout_width="wrap_content"
                 android:layout_height="wrap_content"

                 android:id="@+id/relativeLayout3"
                 app:layout_constraintTop_toTopOf="parent"
                 app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                 app:layout_constraintBottom_toBottomOf="parent">


        <FrameLayout
                android:id="@+id/container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
        />

    </FrameLayout>
    <Button android:id="@+id/editbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Edit Profile"
            android:textSize="15dp"
            android:textAllCaps="true"
            android:layout_marginTop="324dp"
            app:layout_constraintTop_toBottomOf="@+id/username" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="147dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="147dp"></Button>
    <TextView
            android:id="@+id/username"
            android:layout_width="81dp"
            android:layout_height="wrap_content"
            android:text="username"
            android:textSize="18dp"
            android:maxLines="1"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            android:layout_marginTop="6dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="165dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="165dp" android:layout_marginBottom="323dp"
            app:layout_constraintBottom_toTopOf="@+id/editbutton" app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView android:layout_width="373dp"
               android:layout_height="114dp"
               android:src="@drawable/profile"
               android:id="@+id/imageView" app:layout_constraintTop_toTopOf="parent"
               android:layout_marginTop="44dp" app:layout_constraintEnd_toEndOf="parent"
               android:layout_marginEnd="4dp" app:layout_constraintStart_toStartOf="parent"
               android:layout_marginStart="4dp" android:layout_marginBottom="22dp"
               app:layout_constraintBottom_toTopOf="@+id/username"
               app:layout_constraintHorizontal_bias="0.6"/>

    <TextView
            android:text="Profession:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Profession"
            android:layout_marginEnd="310dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="150dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="31dp" android:layout_marginBottom="465dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.65"
            app:layout_constraintVertical_bias="0.217" android:textColor="#0C0C0C" android:textSize="18sp"/>
    <TextView
            android:text="Age"
            android:layout_width="91dp"
            android:layout_height="0dp" android:id="@+id/Age"
            android:layout_marginTop="34dp"
            app:layout_constraintTop_toBottomOf="@+id/Profession" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="323dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.296"
            app:layout_constraintVertical_bias="1.0" android:layout_marginBottom="400dp"/>
    <TextView
            android:text="Education"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Education"
            android:layout_marginTop="90dp"
            app:layout_constraintTop_toBottomOf="@+id/Age" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="322dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="30dp" android:layout_marginBottom="250dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.6"
            app:layout_constraintVertical_bias="0.911" android:textColor="#0B0A0A" android:textSize="18sp"/>
    <TextView
            android:text="Interests"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Interests"
            android:layout_marginTop="34dp"
            app:layout_constraintTop_toBottomOf="@+id/Education" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="327dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.295" android:textColor="#0B0A0A" android:textSize="18sp"
            app:layout_constraintHorizontal_bias="0.166"/>
    <TextView
            android:text="Biznotio-Goals"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/Goals"
            tools:ignore="MissingConstraints"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="18dp"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="35dp"
            app:layout_constraintTop_toBottomOf="@+id/Education" android:layout_marginBottom="34dp"
            app:layout_constraintBottom_toTopOf="@+id/Interests" android:textColor="#1E1D1D" android:textSize="18sp"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="Connect(+)"

            android:layout_width="154dp"
            android:layout_height="40dp" android:id="@+id/Connect"
            tools:ignore="MissingConstraints" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/username" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="147dp" app:layout_constraintStart_toEndOf="@+id/Age"
            android:layout_marginStart="25dp" android:layout_marginBottom="117dp"
            app:layout_constraintBottom_toTopOf="@+id/Goals" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"/>


</androidx.constraintlayout.widget.ConstraintLayout>
