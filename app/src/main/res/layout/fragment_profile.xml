<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/v"
        tools:context=".SettingActivity">


    <ImageView android:layout_width="373dp"
               android:layout_height="114dp"
               android:src="@drawable/profile"
               android:id="@+id/imageView"

               app:layout_constraintBottom_toBottomOf="@+id/imagelogoutbutton"
               app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintVertical_bias="0.099" app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintHorizontal_bias="0.644"/>
    <TextView
            android:text="NAME"
            android:layout_width="103dp"
            android:layout_height="35dp" android:id="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.542"
            app:layout_constraintVertical_bias="0.312"/>
    <ImageButton
            android:id="@+id/imagelogoutbutton"
            android:background="#0B0A0A"

            android:src="@drawable/tinys" tools:ignore="MissingConstraints" android:layout_height="32dp"
            android:layout_width="41dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintVertical_bias="0.94"/>
    <Button
            android:text="connect(+)"
            android:layout_width="151dp"
            android:layout_height="45dp" android:id="@+id/button2"
            android:layout_marginTop="29dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="146dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="150dp" android:layout_marginBottom="203dp"
            app:layout_constraintBottom_toTopOf="@+id/editbutton" app:layout_constraintHorizontal_bias="0.333"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="Edit Profile"

            android:layout_width="137dp"
            android:layout_height="50dp" android:id="@+id/editbutton"
            app:layout_constraintBottom_toBottomOf="@+id/imagelogoutbutton"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.483"
            app:layout_constraintVertical_bias="0.906"/>
    <TextView
            android:text="Interests"
            android:layout_width="91dp"
            android:layout_height="28dp" android:id="@+id/Interests"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.079"
            app:layout_constraintVertical_bias="0.682"/>
    <TextView
            android:text="Profession"
            android:layout_width="117dp"
            android:layout_height="26dp" android:id="@+id/profession"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.115"
            app:layout_constraintVertical_bias="0.499"/>
    <TextView
            android:text="Education"
            android:layout_width="110dp"
            android:layout_height="27dp" android:id="@+id/Education"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.595" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.084"/>
    <TextView
            android:text="BizNotio Goals"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Goals"
            android:layout_marginTop="26dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.08"
            app:layout_constraintVertical_bias="0.744" app:layout_constraintTop_toTopOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>