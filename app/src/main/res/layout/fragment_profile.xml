<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SettingActivity">


    <ImageView android:layout_width="373dp"
               android:layout_height="114dp"
               android:src="@drawable/profile"
               android:id="@+id/imageView" app:layout_constraintTop_toTopOf="parent"
               android:layout_marginTop="44dp" app:layout_constraintEnd_toEndOf="parent"
               android:layout_marginEnd="4dp" app:layout_constraintStart_toStartOf="parent"
               android:layout_marginStart="4dp" android:layout_marginBottom="501dp"

               app:layout_constraintHorizontal_bias="0.6" app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
            android:text="NAME"
            android:layout_width="103dp"
            android:layout_height="35dp" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="190dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="178dp"
            android:layout_marginBottom="448dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="33dp" app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:layout_constraintHorizontal_bias="0.192" app:layout_constraintVertical_bias="0.434"/>
    <ImageButton
            android:id="@+id/imagelogoutbutton"
            android:background="#0B0A0A"

            android:src="@drawable/tinys" tools:ignore="MissingConstraints" android:layout_height="32dp"
            android:layout_width="41dp"
            android:layout_marginTop="3250dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="190dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="180dp" android:layout_marginBottom="49dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.999"/>
    <Button
            android:text="connect(+)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2"
            android:layout_marginTop="29dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="146dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="150dp" android:layout_marginBottom="203dp"
            app:layout_constraintBottom_toTopOf="@+id/editbutton"/>
    <Button
            android:text="Edit Profile"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/editbutton"
            android:layout_marginTop="280dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="146dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="146dp" android:layout_marginBottom="31dp"
            app:layout_constraintBottom_toTopOf="@+id/imagelogoutbutton" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="Profession"
            android:layout_width="117dp"
            android:layout_height="26dp" android:id="@+id/profession"
            android:layout_marginTop="158dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="272dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="317dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="22dp"/>
    <TextView
            android:text="Education"
            android:layout_width="110dp"
            android:layout_height="27dp" android:id="@+id/Education"
            android:layout_marginTop="27dp"
            app:layout_constraintTop_toBottomOf="@+id/profession" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="277dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp" android:layout_marginBottom="263dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Interests"
            android:layout_width="91dp"
            android:layout_height="28dp" android:id="@+id/Interests"
            android:layout_marginTop="22dp"
            app:layout_constraintTop_toBottomOf="@+id/Education" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="291dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="213dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"/>
    <TextView
            android:text="BizNotio Goals"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/Goals"
            android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/Interests" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="168dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="286dp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"/>


</androidx.constraintlayout.widget.ConstraintLayout>